gdi:DatasetSensitiveShape a owl:Ontology, sh:NodeShape;
  rdfs:label "Dataset class - sensitive"@en, "DatasetSensitive"@en;
  rdfs:comment "Excel template for Dataset class - sensitive"@en;
  dct:description "This is an excel template for Dataset class - sensitive for GDI."@en;
  owl:versionInfo "0.1";
  dct:modified "2025-02-10T00:00:00.000Z"^^xsd:dateTime;
  sh:targetClass dcat:Dataset;
  sh:property <http://data.health-ri.nl/GDI/DatasetSensitiveShape#health-theme>, <http://data.health-ri.nl/GDI/DatasetSensitiveShape#provenance>,
    <http://data.health-ri.nl/GDI/DatasetSensitiveShape#publisher>, <http://data.health-ri.nl/GDI/DatasetSensitiveShape#purpose>,
    <http://data.health-ri.nl/GDI/DatasetSensitiveShape#sample>, <http://data.health-ri.nl/GDI/DatasetSensitiveShape#type> .

<http://data.health-ri.nl/GDI/DatasetSensitiveShape#health-theme> sh:path healthdcatap:healthTheme;
  sh:name "health theme"@en;
  sh:description "A category of the Dataset or tag describing the Dataset."@en;
  sh:minCount 1;
  sh:nodeKind sh:IRI;
  dash:viewer dash:URIViewer;
  dash:editor dash:URIEditor .

<http://data.health-ri.nl/GDI/DatasetSensitiveShape#provenance> sh:path dct:provenance;
  sh:name "provenance"@en;
  sh:description "A statement about the lineage of a Dataset."@en;
  sh:minCount 1;
  sh:nodeKind sh:IRI;
  dash:viewer dash:URIViewer;
  dash:editor dash:URIEditor .

<http://data.health-ri.nl/GDI/DatasetSensitiveShape#publisher> sh:path dct:publisher;
  sh:name "publisher"@en;
  sh:description "An entity (organisation) responsible for making the Dataset available."@en;
  sh:minCount 1;
  sh:maxCount 1;
  sh:nodeKind sh:IRI;
  dash:viewer dash:URIViewer;
  dash:editor dash:URIEditor .

<http://data.health-ri.nl/GDI/DatasetSensitiveShape#purpose> sh:path dpv:hasPurpose;
  sh:name "purpose"@en;
  sh:description "Indicates association with Purpose. One (or many) category or sub-category of the purposes can be chosen from the taxonomy provided by dpv and should be provided via IRI, for example https://w3id.org/dpv#ScientificResearch"@en;

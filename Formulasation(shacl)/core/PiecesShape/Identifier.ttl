@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix gdi: <http://data.gdi.eu/core/p2/> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix dash: <http://datashapes.org/dash#> .

gdi:aux-IdentifierShape a owl:Ontology;
  rdfs:label "Identifier"@en;
  rdfs:comment "Health-RI v2 Identifier"@en;
  dct:description "SHACL definitions for GDI v1.1 model for Identifier."@en;
  owl:versionInfo "0.1";
  dct:modified "2025-12-22T14:03:03.240Z"^^xsd:dateTime .

gdi:IdentifierShape a sh:NodeShape;
  rdfs:label "Identifier"@en;
  sh:targetClass adms:Identifier;
  sh:property <http://data.gdi.eu/core/p2/IdentifierShape#skos:notation>, <http://data.gdi.eu/core/p2/IdentifierShape#adms:schemaAgency>, <http://data.gdi.eu/core/p2/IdentifierShape#foaf:name> .

<http://data.gdi.eu/core/p2/IdentifierShape#skos:notation> sh:path skos:notation;
  sh:name "notation"@en;
  sh:description "A string that is an identifier in the context of the identifier scheme referenced by its datatype."@en;
  sh:minCount 1;
  sh:maxCount 1;
  sh:nodeKind sh:Literal;
  sh:datatype xsd:string;
  dash:viewer dash:LiteralViewer;
  dash:editor dash:TextFieldEditor .

<http://data.gdi.eu/core/p2/IdentifierShape#adms:schemaAgency> sh:path adms:schemaAgency;
  sh:name "schema agency"@en;
  sh:description "The name of the agency that issued the identifier."@en;
  sh:nodeKind sh:Literal;
  sh:datatype xsd:string;
  dash:viewer dash:LiteralViewer;
  dash:editor dash:TextFieldEditor .

<http://data.gdi.eu/core/p2/IdentifierShape#foaf:name> sh:path foaf:name;
  sh:name "name"@en;
  sh:description "A name of the agent."@en;
  sh:minCount 1;
  sh:nodeKind sh:Literal;
  sh:uniqueLang true;
  dash:viewer dash:LiteralViewer;
  dash:editor dash:TextAreaEditor .

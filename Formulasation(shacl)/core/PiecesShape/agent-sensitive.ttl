gdi:AgentSensitiveShape a owl:Ontology, sh:NodeShape;
  rdfs:label "Agent class - sensitive"@en, "AgentSensitive"@en;
  rdfs:comment "Excel template for Agent class - sensitive version"@en;
  dct:description "This is an excel template for Agent class sensitive version for GDI."@en;
  owl:versionInfo "0.1";
  dct:modified "2024-02-10T00:00:00.000Z"^^xsd:dateTime;
  sh:targetClass foaf:Agent;
  sh:property <http://data.health-ri.nl/GDI/AgentSensitiveShape#foaf:mbox>, <http://data.health-ri.nl/GDI/AgentSensitiveShape#foaf:name>,
    <http://data.health-ri.nl/GDI/AgentSensitiveShape#foaf:homepage>, <http://data.health-ri.nl/GDI/AgentSensitiveShape#healthdcatap:publishernote>,
    <http://data.health-ri.nl/GDI/AgentSensitiveShape#healthdcatap:publishertype> .

<http://data.health-ri.nl/GDI/AgentSensitiveShape#foaf:mbox> sh:path foaf:mbox;
  sh:name "email"@en;
  sh:description "An email address via which contact can be made. This property SHOULD be used to provide the email address of the Agent, specified using fully qualified mailto: URI scheme. For example: mailto:info@example.com"@en;
  sh:minCount 1;
  sh:maxCount 1;
  sh:nodeKind sh:IRI;
  sh:pattern "^mailto:.+@.+\\..+$";
  dash:viewer dash:URIViewer;
  dash:editor dash:URIEditor .

<http://data.health-ri.nl/GDI/AgentSensitiveShape#foaf:name> sh:path foaf:name;
  sh:name "name"@en;
  sh:description "A name of the agent."@en;
  sh:minCount 1;
  sh:nodeKind sh:Literal;
  dash:viewer dash:LiteralViewer;
  dash:editor dash:TextAreaEditor .

<http://data.health-ri.nl/GDI/AgentSensitiveShape#foaf:homepage> sh:path foaf:homepage;
  sh:name "URL"@en;
  sh:description "A webpage that either allows to make contact (i.e. a webform) or the information contains how to get into contact."@en;
  sh:minCount 1;
  sh:maxCount 1;
  sh:nodeKind sh:IRI;
  dash:viewer dash:URIViewer;
  dash:editor dash:URIEditor .

<http://data.health-ri.nl/GDI/AgentSensitiveShape#healthdcatap:publishernote> sh:path
    healthdcatap:publishernote;
  sh:name "publisher note"@en;
  sh:description "A description of the publisher activities"@en;
  sh:minCount 1;
  sh:maxCount 1;
  sh:nodeKind sh:Literal .

<http://data.health-ri.nl/GDI/AgentSensitiveShape#healthdcatap:publishertype> sh:path
    healthdcatap:publishertype;
  sh:name "publisher type"@en;
  sh:description "A type of organisation that makes the Dataset available"@en;
  sh:minCount 1;
  sh:maxCount 1;
  sh:nodeKind sh:IRI .

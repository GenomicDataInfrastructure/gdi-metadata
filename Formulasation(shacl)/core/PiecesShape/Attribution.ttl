@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix gdi: <http://data.gdi.eu/core/p2/> .
@prefix dash: <http://datashapes.org/dash#> .

gdi:aux-AttributionShape a owl:Ontology;
  rdfs:label "Attribution"@en;
  rdfs:comment "Health-RI v2 Attribution"@en;
  dct:description "SHACL definitions for the Health-RI v2 model for Attribution."@en;
  owl:versionInfo "0.1";
  dct:modified "2025-09-04T13:26:37.470Z"^^xsd:dateTime .

gdi:AttributionShape a sh:NodeShape;
  rdfs:label "Attribution"@en;
  sh:targetClass prov:Attribution;
  sh:property <http://data.gdi.eu/core/p2/AttributionShape#prov:agent>, <http://data.gdi.eu/core/p2/AttributionShape#dcat:hadRole> .

<http://data.gdi.eu/core/p2/AttributionShape#prov:agent> sh:path prov:agent;
  sh:name "agent"@en;
  sh:description "The agent that has a specific role with the described resource."@en;
  sh:minCount 1;
  sh:maxCount 1;
  sh:node gdi:AgentShape;
  dash:viewer dash:DetailsViewer;
  dash:editor dash:BlankNodeEditor .

<http://data.gdi.eu/core/p2/AttributionShape#dcat:hadRole> sh:path dcat:hadRole;
  sh:name "role"@en;
  sh:description "The nature of the relationship between the agent and the described resource, as indicated by a value from the controlled vocabulary."@en;
  sh:minCount 1;
  sh:maxCount 1;
  sh:nodeKind sh:IRI;
  dash:viewer dash:LabelViewer;
  dash:editor dash:URIEditor .
